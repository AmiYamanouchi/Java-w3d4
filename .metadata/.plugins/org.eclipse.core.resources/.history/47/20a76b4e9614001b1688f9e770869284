
public class Driver {

	public static void main(String[] args) {
		// Math.randomはdoubleタイプだから、少数をなくすintにしたい場合は最初に(int)つける
		for(int i = 0 ; i < 5; i++) {
			int randNum = (int)(Math.random()* 10);
			System.out.println(randNum);
		}
		
		double num = 123.45678;
		System.out.println((int)num);

	}

}
